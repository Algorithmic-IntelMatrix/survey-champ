{
  "name": "surveychampfrontend",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev:builder": "bun --cwd apps/builder run dev",
    "dev:runner": "bun --cwd apps/runner run dev",
    "dev": "bun run --filter \"*\" dev",
    "build": "bun run --filter \"*\" build",
    "lint": "bun run --filter \"*\" lint",
    "install:all": "bun install"
  },
  "devDependencies": {
    "bun-types": "^1.3.7",
    "typescript": "^5"
  },
  "dependencies": {
    "wrangler": "^4.61.0"
  }
}