{
  "name": "surveychamp",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "pnpm --filter './apps/*' dev",
    "dev:builder": "pnpm --filter builder dev",
    "dev:runner": "pnpm --filter runner dev",
    "build": "pnpm --filter './apps/*' build",
    "lint": "pnpm --filter './apps/*' lint",
    "install-all": "pnpm install"
  },
  "devDependencies": {
    "bun-types": "^1.3.7",
    "typescript": "^5"
  },
  "dependencies": {
    "wrangler": "^4.61.0"
  }
}