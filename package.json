{
  "name": "surveychamp",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "bun run --filter ./apps/* dev",
    "dev:builder": "bun run --filter builder dev",
    "dev:runner": "bun run --filter runner dev",
    "build": "bun run --filter ./apps/* build",
    "lint": "bun run --filter \"./apps/*\" lint",
    "install": "bun install"
  },
  "devDependencies": {
    "bun-types": "^1.3.7",
    "typescript": "^5"
  },
  "dependencies": {
    "wrangler": "^4.61.0"
  }
}